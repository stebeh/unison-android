root = /mnt/sdcard/DCIM/Camera
root = ssh://me@remotehost.org//home/me/Photos

sshargs = -C
log = true

# upload all Android photos to the /home/me/Photos directory on the remote machine
# (similar to the "Camera upload" feature of Dropbox)
# omit these two options for a standard two-way sync
force = /mnt/sdcard/DCIM/Camera
nodeletion = ssh://me@remotehost.org//home/me/Photos
